record(aSub,"$(P){ACMI:$(NO)}COWFiles-Sub"){
    field(SNAM,"COWFiles")

    field(FTA, "LONG")
    field(NOA, "1")
    field(INPA, "$(P){ACMI:$(NO)}COW:FileIndex-SP")
    field(FTB, "LONG")
    field(NOB, "100")
    field(INPB, "$(P){ACMI:$(NO)}COW:FileIDs-Wfm")
    
    field(FTVA, "STRING")
    field(NOVA, "1")
    field(OUTA, "$(P){ACMI:$(NO)}COW:FileName-I PP")
    field(FTVB, "LONG")
    field(NOVB, "130")
    field(OUTB, "$(P){ACMI:$(NO)}COW:Hist-Wfm PP")
    field(FTVC, "FLOAT")
    field(NOVC, "1")
    field(OUTC, "$(P){ACMI:$(NO)}COW:Hist:Q-I PP")
    field(FTVD, "LONG")
    field(NOVD, "1")
    field(OUTD, "$(P){ACMI:$(NO)}COW:Hist:intg-I PP")
    field(FTVE, "LONG")
    field(NOVE, "1")
    field(OUTE, "$(P){ACMI:$(NO)}COW:Hist:peak-I PP")
    field(FTVF, "LONG")
    field(NOVF, "1")
    field(OUTF, "$(P){ACMI:$(NO)}COW:Hist:fwhm-I PP")
    field(FTVG, "LONG")
    field(NOVG, "1")
    field(OUTG, "$(P){ACMI:$(NO)}COW:Hist:indx-I PP")
    field(FTVH, "LONG")
    field(NOVH, "1")
    field(OUTH, "$(P){ACMI:$(NO)}COW:Hist:base-I PP")
        
}
